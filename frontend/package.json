{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently npm:gen:watch npm:swa",
    "swa": "swa start http://localhost:3000 --run \"vite\" --api-location http://localhost:4000",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "gen": "graphql-codegen --config codegen.yml",
    "gen:watch": "graphql-codegen --config codegen.yml --watch"
  },
  "dependencies": {
    "@aaronpowell/react-static-web-apps-auth": "^1.4.0",
    "@apollo/client": "^3.6.2",
    "@types/node": "^17.0.35",
    "apollo-server-express": "^3.7.0",
    "backend": "^0.0.0",
    "graphql": "^16.5.0",
    "graphql-tag": "^2.12.6",
    "graphql-ws": "^5.8.2",
    "react": "^18.1.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@azure/static-web-apps-cli": "^1.0.1",
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typed-document-node": "2.2.11",
    "@graphql-codegen/typescript": "2.4.11",
    "@graphql-codegen/typescript-operations": "2.4.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "@vitejs/plugin-react": "^1.3.0",
    "concurrently": "^7.2.0",
    "eslint": "^8.15.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "prettier": "^2.6.2",
    "typescript": "^4.6.3",
    "vite": "^2.9.7",
    "vite-plugin-eslint": "^1.6.0"
  }
}
